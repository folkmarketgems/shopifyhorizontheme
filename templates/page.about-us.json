names.custom_liquid","settings":{"custom_liquid":"{% comment %}\n  Instagram Widget for @folkmarketgems\n  Save as: instagram-feed.liquid\n{% endcomment %}\n\n{{ 'instafeed.min.js' | asset_url | script_tag }}\n\n<style>\n  .instagram-container {\n    padding: 40px 20px;\n    background: #f8f8f8;\n    margin: 2rem 0;\n  }\n  .instagram-title {\n    text-align: center;\n    margin-bottom: 25px;\n  }\n  .instagram-title h2 {\n    font-size: 28px;\n    margin: 0 0 10px;\n    color: #333;\n  }\n  .instagram-title a {\n    color: #E1306C;\n    text-decoration: none;\n    font-weight: 500;\n  }\n  .instagram-feed {\n    display: flex;\n    overflow-x: auto;\n    gap: 15px;\n    padding: 10px 0;\n    scrollbar-width: thin;\n  }\n  .instagram-post {\n    flex: 0 0 280px;\n    border-radius: 12px;\n    overflow: hidden;\n    position: relative;\n    transition: transform 0.3s ease;\n  }\n  .instagram-post:hover {\n    transform: translateY(-5px);\n  }\n  .instagram-image {\n    width: 100%;\n    height: 280px;\n    object-fit: cover;\n    border-radius: 12px;\n  }\n  \n  \/* Scrollbar Styling *\/\n  .instagram-feed::-webkit-scrollbar {\n    height: 8px;\n  }\n  .instagram-feed::-webkit-scrollbar-track {\n    background: #f1f1f1;\n    border-radius: 4px;\n  }\n  .instagram-feed::-webkit-scrollbar-thumb {\n    background: #888;\n    border-radius: 4px;\n  }\n<\/style>\n\n<div class=\"instagram-container\">\n  <div class=\"instagram-title\">\n    <h2>Follow Us @folkmarketgems<\/h2>\n    <a href=\"https:\/\/www.instagram.com\/folkmarketgems\" target=\"_blank\" rel=\"noopener noreferrer\">\n      View Profile →\n    <\/a>\n  <\/div>\n  <div id=\"instafeed\" class=\"instagram-feed\"><\/div>\n<\/div>\n\n<script>\n  document.addEventListener('DOMContentLoaded', function() {\n    const userId = '17841406338772949'; \/\/ Your Instagram User ID (get from https:\/\/smashballoon.com\/instagram-feed\/find-instagram-user-id\/)\n    const accessToken = 'IGQWRO...'; \/\/ Your Instagram Access Token\n\n    const feed = new Instafeed({\n      accessToken: accessToken,\n      userId: userId,\n      target: 'instafeed',\n      limit: 12,\n      resolution: 'standard_resolution',\n      template: `\n        <a href=\"{{link}}\" target=\"_blank\" rel=\"noopener noreferrer\" class=\"instagram-post\">\n          <img src=\"{{image}}\" alt=\"{{caption}}\" class=\"instagram-image\" loading=\"lazy\">\n        <\/a>\n      `,\n      filter: function(image) {\n        return image.type === 'image' || image.type === 'carousel';\n      },\n      after: function() {\n        \/\/ Add fallback for broken images\n        document.querySelectorAll('.instagram-image').forEach(img => {\n          img.onerror = function() {\n            this.parentElement.remove();\n          }\n        });\n      }\n    });\n\n    \/\/ Run feed with error handling\n    feed.run().catch(error => {\n      console.error('Instagram Feed Error:', error);\n      document.getElementById('instafeed').innerHTML = `\n        <div style=\"text-align:center; width:100%; padding:20px; color:#666\">\n          Follow us on Instagram to see our latest posts!\n        <\/div>\n      `;\n    });\n  });\n<\/script>","color_scheme":"","section_width":"page-width","padding-block-start":0,"padding-block-end":0}}},"order":["hero_Yq4iEN","section_9dNifq","main","section_7Ewqmd","media_with_content_qtnL9H","section_hnjW4Q","section_4369B9","custom_liquid_weJzLi"]}